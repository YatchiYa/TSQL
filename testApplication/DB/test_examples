select openig , closing from stocks
select opening[15:30] from stocks
select opening[15:30] > 30 from stocks
select opening[15:30] > 25, closing [25 : 36] from stocks
select opening > 30 when closing < 65 from stocks
select opening where opening > 15 from stocks
select opening from stocks where opening > 15
select opening > 20, headerquaters when closing[15:30] < 30 from stocks where headerqauters = paris
select opening > 20, headerquaters when closing [15:30] < 30 where headerqauters = paris from stocks 











